                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{searchTypeKeywordVM.searchTypeKeywordModel.txtOliviaRodrigo}"
                style="@style/txtUrbanistromanmedium18_4" tools:text="@string/lbl_olivia_rodrigo"/>
            <ImageView android:id="@+id/imageCloseFour"
                android:layout_height="@dimen/_13pxh"
                android:layout_width="@dimen/_13pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_close_13X13"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close_13X13"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearAutoLayoutHor6"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtTheWeeknd"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{searchTypeKeywordVM.searchTypeKeywordModel.txtTheWeeknd}"
                style="@style/txtUrbanistromanmedium18_4" tools:text="@string/lbl_the_weeknd"/>
            <ImageView android:id="@+id/imageCloseFive"
                android:layout_height="@dimen/_13pxh"
                android:layout_width="@dimen/_13pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_close_13X13"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close_13X13"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearAutoLayoutHor7"
