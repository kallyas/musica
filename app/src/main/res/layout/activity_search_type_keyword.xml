<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="searchTypeKeywordVM" type="com.musica.app.modules.searchtypekeyword.data.viewmodel.SearchTypeKeywordVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearSearchTypeKey"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.SearchView
            android:background="@drawable/rectangle_bg_gray_52_border_red_a700_radius_16"
            android:gravity="center_horizontal"
            android:id="@+id/searchViewSearch"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:theme="@style/searchViewSearch"
            app:iconifiedByDefault="false"
            app:queryBackground="@android:color/transparent"
            app:queryHint="@string/lbl_abcdefghijklm2" app:searchIcon="@drawable/img_search_red_A700"/>
        <LinearLayout android:id="@+id/linearAutoLayoutHor"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtRecentSearches"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{searchTypeKeywordVM.searchTypeKeywordModel.txtRecentSearches}"
                style="@style/txtUrbanistromanbold20" tools:text="@string/lbl_recent_searches"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtClearAll"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{searchTypeKeywordVM.searchTypeKeywordModel.txtClearAll}"
                style="@style/txtUrbanistromanbold16" tools:text="@string/lbl_clear_all"/>
        </LinearLayout>
        <View android:background="@color/bluegray_100"
            android:id="@+id/lineLine"
            android:layout_gravity="center_horizontal"
            android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_380pxh"/>
        <LinearLayout android:id="@+id/linearAutoLayoutHor1"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtArianaGrande"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{searchTypeKeywordVM.searchTypeKeywordModel.txtArianaGrande}"
                style="@style/txtUrbanistromanmedium18_4" tools:text="@string/lbl_ariana_grande"/>
            <ImageView android:id="@+id/imageClose"
                android:layout_height="@dimen/_13pxh"
                android:layout_width="@dimen/_13pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_close_13X13"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close_13X13"/>
        </LinearLayout>
        <LinearLayout android:id="@+id/linearAutoLayoutHor2"
            android:layout_height="wrap_content"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end"
                android:id="@+id/txtMorganWallen"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:letterSpacing="0.01" android:singleLine="true"
                android:text="@{searchTypeKeywordVM.searchTypeKeywordModel.txtMorganWallen}"
                style="@style/txtUrbanistromanmedium18_4" tools:text="@string/lbl_morgan_wallen"/>
            <ImageView android:id="@+id/imageCloseOne"
                android:layout_height="@dimen/_13pxh"
