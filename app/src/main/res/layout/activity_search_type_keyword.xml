                style="@style/txtUrbanistromanmedium18_4" tools:text="@string/lbl_juice_wrld"/>
            <ImageView android:id="@+id/imageCloseSeven"
                android:layout_height="@dimen/_13pxh"
                android:layout_width="@dimen/_13pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_close_13X13"
                tools:ignore="ContentDescription" tools:src="@drawable/img_close_13X13"/>
        </LinearLayout>
    </LinearLayout>
</layout>
