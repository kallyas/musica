<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listtodayRowModel" type="com.musica.app.modules.songsnotifications.data.model.ListtodayRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowtoday"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageImage"
            android:layout_height="@dimen/_80pxh"
            android:layout_width="@dimen/_80pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_20pxh}"
            imageRes="@{@drawable/img_image}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearAutoLayoutVer"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAutoLayoutHor1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtToday"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listtodayRowModel.txtToday}"
                    style="@style/txtUrbanistromanbold12_1" tools:text="@string/lbl_today"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSixtyOne"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listtodayRowModel.txtSixtyOne}"
                    style="@style/txtUrbanistromanbold12_1" tools:text="@string/lbl"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTime"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listtodayRowModel.txtTime}"
                    style="@style/txtUrbanistromanbold12_1" tools:text="@string/lbl_04_36_mins"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtBREAKMYSOUL"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{listtodayRowModel.txtBREAKMYSOUL}"
                style="@style/txtUrbanistromanregular18_2" tools:text="@string/lbl_break_my_soul"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAutoLayoutHor2"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBeyonce"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listtodayRowModel.txtBeyonce}"
                    style="@style/txtUrbanistromanbold12_1" tools:text="@string/lbl_beyonce"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSixtyTwo"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listtodayRowModel.txtSixtyTwo}"
                    style="@style/txtUrbanistromanbold12_1" tools:text="@string/lbl"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAlbum"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listtodayRowModel.txtAlbum}"
                    style="@style/txtUrbanistromanbold12_1" tools:text="@string/lbl_album"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageExclude"
            android:layout_height="@dimen/_26pxh"
            android:layout_width="@dimen/_26pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_exclude"
            tools:ignore="ContentDescription" tools:src="@drawable/img_exclude"/>
        <ImageView android:id="@+id/imageVector"
            android:layout_height="@dimen/_13pxh"
            android:layout_width="@dimen/_3pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_vector_gray_900"
            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_900"/>
    </LinearLayout>
</layout>
