<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listyesterdayOneRowModel" type="com.musica.app.modules.songsnotifications.data.model.ListyesterdayOneRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowyesterday"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageImageThree"
            android:layout_height="@dimen/_80pxh"
            android:layout_width="@dimen/_80pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_20pxh}"
            imageRes="@{@drawable/img_image_70}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image_70"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearAutoLayoutVer"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAutoLayoutHor1"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtYesterdayOne"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listyesterdayOneRowModel.txtYesterdayOne}"
                    style="@style/txtUrbanistromanbold12_1" tools:text="@string/lbl_yesterday"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSixtySeven"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listyesterdayOneRowModel.txtSixtySeven}"
                    style="@style/txtUrbanistromanbold12_1" tools:text="@string/lbl"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTimeThree"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listyesterdayOneRowModel.txtTimeThree}"
                    style="@style/txtUrbanistromanbold12_1" tools:text="@string/lbl_04_36_mins"/>
            </LinearLayout>
            <TextView android:ellipsize="end"
                android:id="@+id/txtSharks"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{listyesterdayOneRowModel.txtSharks}"
                style="@style/txtUrbanistromanregular18_2" tools:text="@string/lbl_sharks"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAutoLayoutHor2"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtBeyonceOne"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listyesterdayOneRowModel.txtBeyonceOne}"
                    style="@style/txtUrbanistromanbold12_1" tools:text="@string/lbl_beyonce"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSixtyEight"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listyesterdayOneRowModel.txtSixtyEight}"
                    style="@style/txtUrbanistromanbold12_1" tools:text="@string/lbl"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtAlbumOne"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listyesterdayOneRowModel.txtAlbumOne}"
                    style="@style/txtUrbanistromanbold12_1" tools:text="@string/lbl_album"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageExcludeThree"
            android:layout_height="@dimen/_26pxh"
            android:layout_width="@dimen/_26pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_exclude"
            tools:ignore="ContentDescription" tools:src="@drawable/img_exclude"/>
        <ImageView android:id="@+id/imageVectorThree"
            android:layout_height="@dimen/_13pxh"
            android:layout_width="@dimen/_3pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_vector_gray_900"
            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_900"/>
    </LinearLayout>
</layout>
