<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="listsongtitleTwoRowModel" type="com.musica.app.modules.searchresulttop.data.model.ListsongtitleTwoRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowsongtitle"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageImageFive"
            android:layout_height="@dimen/_80pxh"
            android:layout_width="@dimen/_80pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_20pxh}"
            imageRes="@{@drawable/img_image_52}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image_52"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearAutoLayoutVer"
            android:layout_height="wrap_content"
            android:layout_width="wrap_content" android:orientation="vertical">
            <TextView android:ellipsize="end"
                android:id="@+id/txtSongTitleTwo"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@{listsongtitleTwoRowModel.txtSongTitleTwo}"
                style="@style/txtUrbanistromanbold18" tools:text="@string/lbl_firework_cover"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearAutoLayoutHor"
                android:layout_height="wrap_content"
                android:layout_width="wrap_content" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDetailsSeven"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listsongtitleTwoRowModel.txtDetailsSeven}"
                    style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_the_sappear"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFiftySix"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listsongtitleTwoRowModel.txtFiftySix}"
                    style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtDetailsOne"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:letterSpacing="0.01"
                    android:singleLine="true"
                    android:text="@{listsongtitleTwoRowModel.txtDetailsOne}"
                    style="@style/txtUrbanistromanmedium12" tools:text="@string/lbl_song"/>
            </LinearLayout>
        </LinearLayout>
        <ImageView android:id="@+id/imageExcludeThree"
            android:layout_height="@dimen/_26pxh"
            android:layout_width="@dimen/_26pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_exclude"
            tools:ignore="ContentDescription" tools:src="@drawable/img_exclude"/>
        <ImageView android:id="@+id/imageVectorFive"
            android:layout_height="@dimen/_13pxh"
            android:layout_width="@dimen/_3pxh"
            android:scaleType="fitXY"
            android:src="@drawable/img_vector_gray_900"
            tools:ignore="ContentDescription" tools:src="@drawable/img_vector_gray_900"/>
    </LinearLayout>
</layout>
